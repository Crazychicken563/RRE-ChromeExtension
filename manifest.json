{
    "manifest_version": 2,

    "name": "RRE For Google Chrome",
    "version": "1.0",
    "description": "RRE Chrome Extension",
    "permissions": ["<all_urls>"],

    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["main.js"],
        "matches": ["https://www.reddit.com/*"]
    }]
}
